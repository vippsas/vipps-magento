<klarna xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Klarna_Core:etc/klarna.xsd">
    <external_payment_method id="vipps" translate="label description" ifconfig="payment/vipps/active">
        <label>Vipps</label>
        <name>Vipps</name>
        <redirect_url><![CDATA[{{secure_base_url}}vipps/payment/klarna_initRegular]]></redirect_url>
        <image_url><![CDATA[https://raw.githubusercontent.com/vippsas/vipps-magento/master/view/base/web/images/vipps_logo_69x24.png]]></image_url>
        <description>Checkout using Vipps.</description>
        <fee>0</fee>
    </external_payment_method>
</klarna>
